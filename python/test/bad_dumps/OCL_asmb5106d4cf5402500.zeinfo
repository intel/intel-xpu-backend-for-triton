        addrmode:        stateless
        addrspace:       global
        access_type:     readwrite
      - arg_type:        arg_bypointer
        offset:          64
        size:            8
        arg_index:       4
        addrmode:        stateless
        addrspace:       global
        access_type:     readwrite
      - arg_type:        arg_bypointer
        offset:          72
        size:            8
        arg_index:       5
        addrmode:        stateless
        addrspace:       global
        access_type:     readwrite
      - arg_type:        private_base_stateless
        offset:          80
        size:            8
    per_thread_payload_arguments:
      - arg_type:        local_id
        offset:          0
        size:            192
