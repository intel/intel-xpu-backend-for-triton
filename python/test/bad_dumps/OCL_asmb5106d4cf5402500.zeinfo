---
version:         '1.53'
kernels:
  - name:            kernel
    user_attributes:
      intel_reqd_sub_group_size: 32
      intel_reqd_workgroup_walk_order: [ 0, 1, 2 ]
      reqd_work_group_size: [ 128, 1, 1 ]
    execution_env:
      disable_mid_thread_preemption: true
      grf_count:       128
      has_4gb_buffers: true
      has_no_stateless_write: true
      offset_to_skip_per_thread_data_load: 256
      required_work_group_size: [ 128, 1, 1 ]
      simd_size:       32
      subgroup_independent_forward_progress: true
      eu_thread_count: 8
    payload_arguments:
      - arg_type:        global_id_offset
        offset:          0
        size:            12
      - arg_type:        local_size
        offset:          12
        size:            12
      - arg_type:        arg_bypointer
        offset:          32
        size:            8
        arg_index:       0
        addrmode:        stateless
        addrspace:       global
        access_type:     readwrite
      - arg_type:        arg_bypointer
        offset:          40
        size:            8
        arg_index:       1
        addrmode:        stateless
        addrspace:       global
        access_type:     readwrite
      - arg_type:        arg_bypointer
        offset:          48
